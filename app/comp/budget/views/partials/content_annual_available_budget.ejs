                            <td>
                                <input value="EC$ <%- retValues.budget.ammount*(retValues.budget.Routine/100)*(retValues.budget.Brushcutting/100) %>" class="form-control"
                                    id="disabledInputBrushcutting" type="text" placeholder="EC$ 0.00" disabled="">
                            </td>
                        </tr>
                        <tr>
                            <td>Drain clearing</td>
                            <td>
                                <input value="EC$ <%- retValues.budget.ammount*(retValues.budget.Routine/100)*(retValues.budget.Drainclearing/100) %>" class="form-control"
                                    id="disabledInputDrainclearing" type="text" placeholder="EC$ 0.00" disabled="">

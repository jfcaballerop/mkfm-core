<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>

        <style>
            /* Always set the map height explicitly to define the size of the div
             * element that contains the map. */

            #map {
                height: 100%;
            }
            /* Optional: Makes the sample page fill the window. */

            html,
            body {
                height: 100%;
                margin: 0;
                padding: 0;
            }

            #info-box {
                background-color: white;
                border: 1px solid black;
                bottom: 30px;
                height: 30px;
                padding: 5px;
                position: absolute;
                left: 30px;
            }
        </style>
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <% include partials/header %>
        </header>
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <% include partials/left_menu %>

        </aside>
        <!--sidebar end-->
        <!-- main content Permission denied -->
        <% if(!token.admin) {%>
            <% include partials/error_permissions %>
                <!-- End permission denied -->
                <%} else {%>
                    <!--main content start-->
                    <section id="main-content">
                        <section class="wrapper site-min-height">
                            <div class="row mt">
                                <div class="col-md-12">
                                    <h3>
                                        <i class="fa fa-angle-right"></i>
                                        <%- trans('MENU_PANEL_COST_LIBRARY') %>
                                    </h3>
                                </div>
                            </div>
                            <!-- page start-->
                            <div class="row mt" id="">
                                <div class="col-md-12 mt">
                                    <div class="content-panel">
                                        <h4>
                                            <i class="fa fa-angle-right"></i>
                                            <%- trans('COST_LIBRARY_TITLE') %>
                                        </h4>
                                        <hr>
                                        <table class="table table-hover">


                                            <thead>
                                                <tr>
                                                    <th>
                                                        <%- trans('CODE') %>
                                                    </th>
                                                    <th>
                                                        <%- trans('COST_LIBRARY_HEAD_ASSET') %>
                                                    </th>
                                                    <th>
                                                        <%- trans('COST_LIBRARY_HEAD_UNIT_PRICE') %>
                                                    </th>
                                                    <th>
                                                        <%- trans('VALUE') %>
                                                    </th>
                                                </tr>
                                            </thead>
                                            <tbody id="costLibraryTable">

                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>


                            <!-- page end-->
                        </section>
                    </section>
                    <!-- /MAIN CONTENT -->
                    <% } %>
                        <!--main content end-->
                        <!--footer start-->
                        <footer class="site-footer">
                            <% include partials/footer %>
                        </footer>
                        <!--footer end-->
    </section>

    <% include partials/footer_scripts %>
        <!-- js placed at the end of the document so the pages load faster -->
        <!-- <script src="/assets/js/jquery.js"></script> -->
        <script src="/assets/js/bootstrap.min.js"></script>
        <script class="include" type="text/javascript" src="/assets/js/jquery.dcjqaccordion.2.7.js"></script>
        <script src="/assets/js/jquery.scrollTo.min.js"></script>
        <script src="/assets/js/jquery.nicescroll.js" type="text/javascript"></script>


        <!--common script for all pages-->
        <script src="/assets/js/common-scripts.js"></script>


        <!--script for this page-->
        <%- include('partials/costs_library_scripts'); %>
            <script src="/assets/js/jquery.table2excel.min.js"></script>
            <script src="/assets/js/gmaps/gmaps.js"></script>



</body>

</html>
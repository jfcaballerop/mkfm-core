<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>

        <style>
            /* Always set the map height explicitly to define the size of the div
             * element that contains the map. */

            #map {
                height: 100%;
            }
            /* Optional: Makes the sample page fill the window. */

            html,
            body {
                height: 100%;
                margin: 0;
                padding: 0;
            }

            #info-box {
                background-color: white;
                border: 1px solid black;
                bottom: 30px;
                height: 30px;
                padding: 5px;
                position: absolute;
                left: 30px;
            }
        </style>
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <% include partials/header %>
        </header>
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <% include partials/left_menu %>

        </aside>
        <!--sidebar end-->
        <!-- main content Permission denied -->
        <% if(!token.admin) {%>
            <% include partials/error_permissions %>
                <!-- End permission denied -->
                <%} else {%>
                    <!--main content start-->
                    <section id="main-content">
                        <section class="wrapper site-min-height">
                            <div class="row mt">
                                <div class="col-md-12">
                                    <h3>
                                        <i class="fa fa-angle-right"></i>
                                        <%- trans('MENU_PANEL_BUDGET') %>
                                    </h3>
                                </div>
                            </div>
                            <!-- page start-->
                            <div class="row mt" id="">
                                <div class="col-md-2">
                                    <div class="form-panel">
                                        <div class="row mt">
                                            <!-- Budget Criteria -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Budget Criteria
                                                </h5>
                                                <div class="checkbox">
                                                    <label for="Globalbudget">
                                                        <input type="checkbox" id="Globalbudget" value="Globalbudget"> Global budget
                                                    </label>
                                                    <br>
                                                    <label for="Annualavailablebudget">
                                                        <input type="checkbox" id="Annualavailablebudget" value="Annualavailablebudget"> Annual available budget
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row mt">
                                            <!-- Maintenance activities -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Maintenance activities
                                                </h5>
                                                <div class="row mt">
                                                    <div class="col-md-6">
                                                        <div class="checkbox">
                                                            <label for="Routine">Routine
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <input type="text" id="Routine" class="form-control round-form">
                                                    </div>

                                                </div>
                                                <div class="row mt">
                                                    <div class="col-md-6">
                                                        <div class="checkbox">
                                                            <label for="Periodic">Periodic
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <input type="text" id="Periodic" class="form-control round-form">
                                                    </div>

                                                </div>
                                                <div class="row mt">
                                                    <div class="col-md-6">
                                                        <div class="checkbox">
                                                            <label for="Emergency">Emergency
                                                            </label>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-6">
                                                        <input type="text" id="Emergency" class="form-control round-form">
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <hr>

                                        <div class="row mt">
                                            <!-- Risk Criteria -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Risk Criteria
                                                </h5>
                                                <div class="checkbox">
                                                    <label for="PhysicalDeterioration">
                                                        <input type="checkbox" id="PhysicalDeterioration" value="PhysicalDeterioration"> Physical Deterioration
                                                    </label>
                                                    <br>
                                                    <label for="NaturalHazards">
                                                        <input type="checkbox" id="NaturalHazards" value="NaturalHazards"> Natural Hazards
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row mt">
                                            <!-- Acceptable Level of Risk -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Acceptable Level of Risk
                                                </h5>
                                                <div class="row mt">
                                                    <div class="col-md-12">
                                                        <input type="text" id="AcceptableLevelofRisk" class="form-control round-form">
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row mt">
                                            <!-- Investment Period -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Investment Period
                                                </h5>
                                                <div class="checkbox">
                                                    <label for="1year">
                                                        <input type="checkbox" id="1year" value="1year"> 1 year
                                                    </label>
                                                    <br>
                                                    <label for="5years">
                                                        <input type="checkbox" id="5years" value="5years"> 5 years
                                                    </label>
                                                    <br>
                                                    <label for="10years">
                                                        <input type="checkbox" id="10years" value="10years"> 10 years
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row mt">
                                            <!-- Investment Strategies -->
                                            <div class="col-md-12">
                                                <h5 class="mb">
                                                    <i class="fa fa-angle-right"></i> Investment Strategies
                                                </h5>
                                                <div class="checkbox">
                                                    <label for="Distancesinergies">
                                                        <input type="checkbox" id="Distancesinergies" value="Distancesinergies"> Distance sinergies
                                                    </label>
                                                    <br>
                                                    <label for="Assetsshare">
                                                        <input type="checkbox" id="Assetsshare" value="Assetsshare"> Assets share
                                                    </label>
                                                    <br>
                                                    <label for="Corridorpriorization">
                                                        <input type="checkbox" id="Corridorpriorization" value="Corridorpriorization"> Corridor priorization
                                                    </label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <!-- PARTE DCHA DE RESULTADOS-->
                                <div class="col-md-10">
                                    <div class="form-panel">
                                        <div class="row mt">
                                            <div class="col-md-12">
                                                <div class="btn-group btn-group-justified">
                                                    <div class="btn-group">
                                                        <button type="button" class="btn btn-theme">Summary</button>
                                                    </div>
                                                    <div class="btn-group">
                                                        <button type="button" class="btn btn-theme">Priorization of Interventions (FY2018)</button>
                                                    </div>
                                                    <div class="btn-group">
                                                        <button type="button" class="btn btn-theme">Priorization of Inspections (FY2018)</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>


                                </div>
                            </div>
                            <div class="row mt">

                            </div>

                            <!-- page end-->
                        </section>
                    </section>
                    <!-- /MAIN CONTENT -->
                    <% } %>
                        <!--main content end-->
                        <!--footer start-->
                        <footer class="site-footer">
                            <% include partials/footer %>
                        </footer>
                        <!--footer end-->
    </section>

    <% include partials/footer_scripts %>
        <!-- js placed at the end of the document so the pages load faster -->
        <!-- <script src="/assets/js/jquery.js"></script> -->
        <script src="/assets/js/bootstrap.min.js"></script>
        <script class="include" type="text/javascript" src="/assets/js/jquery.dcjqaccordion.2.7.js"></script>
        <script src="/assets/js/jquery.scrollTo.min.js"></script>
        <script src="/assets/js/jquery.nicescroll.js" type="text/javascript"></script>


        <!--common script for all pages-->
        <script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
        <script src="/assets/js/common-scripts.js"></script>


        <!--script for this page-->
        <script async defer src="https://maps.googleapis.com/maps/api/js?key=<%= api_key %>"></script>

        <%- include('partials/query_scripts'); %>
            <script src="/assets/js/jquery.table2excel.min.js"></script>
            <script src="/assets/js/gmaps/gmaps.js"></script>



</body>

</html>
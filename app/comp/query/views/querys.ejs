<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>

        <style>
            /* Always set the map height explicitly to define the size of the div
             * element that contains the map. */
            
            #map {
                height: 100%;
            }
            /* Optional: Makes the sample page fill the window. */
            
            html,
            body {
                height: 100%;
                margin: 0;
                padding: 0;
            }
            
            #info-box {
                background-color: white;
                border: 1px solid black;
                bottom: 30px;
                height: 30px;
                padding: 5px;
                position: absolute;
                left: 30px;
            }
        </style>
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <% include partials/header %>
        </header>
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <% include partials/left_menu %>

        </aside>
        <!--sidebar end-->
        <!-- main content Permission denied -->
        <% if(!token.admin) {%>
            <% include partials/error_permissions %>
                <!-- End permission denied -->
                <%} else {%>
                    <!--main content start-->
                    <section id="main-content">
                        <section class="wrapper site-min-height">
                            <div class="row mt">
                                <div class="col-md-12">
                                    <h3><i class="fa fa-angle-right"></i>
                                        <%- trans('MENU_PANEL_QUERY') %>
                                    </h3>
                                </div>
                            </div>
                            <!-- page start-->
                            <div class="row mt">
                                <div class="col-md-3">
                                    <div class="form-panel">
                                        <h5 class="mb"><i class="fa fa-angle-right"></i>
                                            <%- trans('QUERYS_FILTER') %>
                                        </h5>
                                        <ul id="ListSwitchOption" class="list-group">

                                            <% for (var key in filters) { %>
                                                <li class="list-group-item">

                                                    <input type="checkbox" id="SwitchOption<%- key %>" name="SwitchOption<%- key %>" data-toggle="toggle" data-onstyle="success" data-size="mini">
                                                    <label for="SwitchOption<%- key %>"><%- key %></label>


                                                </li>

                                                <% }; %>
                                        </ul>
                                    </div>
                                </div>
                                <div class="col-md-9">
                                    <div class="form-panel">
                                        <h6 class="mb"><i class="fa fa-angle-right"></i> No se ha aplicado ningún filtro. Se encontraron 1277 registros. ￼
                                        </h6>


                                    </div>
                                </div>
                            </div>
                            <div class="row mt">

                            </div>

                            </div>
                            <!-- page end-->
                        </section>
                    </section>
                    <!-- /MAIN CONTENT -->
                    <%} %>
                        <!--main content end-->
                        <!--footer start-->
                        <footer class="site-footer">
                            <% include partials/footer %>
                        </footer>
                        <!--footer end-->
    </section>

    <% include partials/footer_scripts %>
        <!-- js placed at the end of the document so the pages load faster -->
        <!-- <script src="/assets/js/jquery.js"></script> -->
        <script src="/assets/js/bootstrap.min.js"></script>
        <script class="include" type="text/javascript" src="/assets/js/jquery.dcjqaccordion.2.7.js"></script>
        <script src="/assets/js/jquery.scrollTo.min.js"></script>
        <script src="/assets/js/jquery.nicescroll.js" type="text/javascript"></script>


        <!--common script for all pages-->
        <script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>
        <script src="/assets/js/common-scripts.js"></script>


        <!--script for this page-->

        <%- include('partials/query_scripts'); %>


</body>

</html>
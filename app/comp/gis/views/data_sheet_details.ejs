<!DOCTYPE html>
<html lang="en">

<head>
    <% include partials/head %>
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <% include partials/header %>
        </header>
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <% include partials/left_menu %>

        </aside>
        <!--sidebar end-->

        <!-- **********************************************************************************************************************************************************
        ***** Google Maps
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper site-min-height">
                <div class="row mt">
                    <div class="col-md-10">
                        <h3>
                            <i class="fa fa-angle-right"></i>
                            <%- trans('MENU_DATASHEET') %>
                        </h3>
                    </div>

                </div>
                <!-- page start-->

                <div class="row mt">
                    <div class="col-md-2">
                        <div class="form-panel">
                            <h5 class="mb">
                                <i class="fa fa-angle-right"></i>
                                <%- trans('DSHEET_ASSET_CODE') %>
                            </h5>
                            <input id="searchtool" type="text" list="json-datalist" class="form-control round-form" placeholder="Enter road/asset code"
                                value="<%- assetdata.properties.asset_code %>" disabled>
                            <datalist id="json-datalist"></datalist>
                            <div id="error" class="alert alert-danger" role="alert" style="display:none;"></div>
                            <div id="response" class="alert alert-success" role="alert"> Asset Type:
                                <%- assetdata.properties.asset_type %>
                            </div>

                        </div>
                        <div id="datainfo" class="form-panel">
                            <h5 class="mb">
                                <i class="fa fa-angle-right"></i>
                                <%- trans('DSHEET_DATA_INFO') %>
                            </h5>
                            <div class="checkbox">
                                <label>
                                    <input id="generaldata" type="checkbox" value="General Data">General Data</label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input id="inventorydata" type="checkbox" value="Inventory Data">Inventory Data</label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input id="operation" type="checkbox" value="Operation and Maintenance Data">Operation and Maintenance Data</label>
                            </div>
                            <div class="checkbox">
                                <label>
                                    <input id="risk" type="checkbox" value="Indexes and risk analysis">Indexes and risk analysis</label>
                            </div>

                        </div>
                        <div id="dataupload" class="form-panel">
                            <h5 class="mb">
                                <i class="fa fa-angle-right"></i>
                                <%- trans('DSHEET_DATA_UPLOAD') %>
                            </h5>
                            <div class="checkbox">
                                <label>
                                    <input id="generaldataupload" type="checkbox" value="General Data">File Upload</label>
                            </div>
                        </div>
                        <% if(assetdata.properties.asset_photos !== undefined){ %>
                            <div id="datainfophoto" class="form-panel">
                                <% assetdata.properties.asset_photos.forEach(function(foto){ %>
                                    <img src="<%- foto %>" class="img-rounded" alt="foto">
                                    <% }); %>
                            </div>
                            <% } %>



                    </div>
                    <div class="col-md-4">
                        <div id="divmap1" style="display: none;">

                        </div>
                        <div id="generaldatadiv" class="content-panel" style="display: none;">
                            <table class="table table-striped table-advance table-hover">
                                <thead>
                                    <tr>
                                        <th colspan="2" class="text-center">General Data</th>
                                        <tr>
                                </thead>
                                <tbody id="generaldatatbody">
                                </tbody>
                            </table>
                        </div>
                        <div id="inventorydatadiv" class="content-panel" style="display: none;">
                            <table class="table table-striped table-advance table-hover">
                                <thead>
                                    <tr>
                                        <th colspan="2" class="text-center">Inventory Data</th>
                                        <tr>
                                </thead>
                                <tbody id="inventorydatabody">
                                </tbody>
                            </table>
                        </div>
                        <div id="operationdiv" class="content-panel" style="display: none;">
                            <table class="table table-striped table-advance table-hover">
                                <thead>
                                    <tr>
                                        <th colspan="2" class="text-center">Operation Data</th>
                                        <tr>
                                </thead>
                                <tbody id="operationdatabody">
                                </tbody>
                            </table>
                        </div>
                        <div id="riskdiv" class="content-panel" style="display: none;">
                            <table class="table table-striped table-advance table-hover">
                                <thead>
                                    <tr>
                                        <th colspan="2" class="text-center">Indexes and risk analysis</th>
                                        <tr>
                                </thead>
                                <tbody id="riskdatabody">
                                </tbody>
                            </table>
                        </div>
                    </div>
                    <div class="col-md-6">
                        <div id="rowmap" class="row">

                        </div>
                    </div>


                    <!-- <iframe allowfullscreen width="100%" height="100%" frameborder="0" style="border:0" src="https://www.google.com/maps/embed/v1/place?q=place_id:ChIJFVoV8vrSFIwRPP06BQmRs0k&key=<%= api_key %>&maptype=satellite" allowfullscreen> -->
                    <!-- </iframe> -->
                </div>
                </div>
                <!-- page end-->
            </section>
        </section>
        <!-- /MAIN CONTENT -->

        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <% include partials/footer %>
        </footer>
        <!--footer end-->
    </section>

    <% include partials/footer_scripts %>
        <!-- js placed at the end of the document so the pages load faster -->

        <!--common script for all pages-->
        <!-- <script src="//cdnjs.cloudflare.com/ajax/libs/jquery.form/3.51/jquery.form.min.js"></script> -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.form/4.2.2/jquery.form.min.js" integrity="sha384-FzT3vTVGXqf7wRfy8k4BiyzvbNfeYjK+frTVqZeNDFl8woCbF0CYG6g2fMEFFo/i"
            crossorigin="anonymous"></script>


        <script src="/assets/js/moment-with-locales.min.js"></script>
        <!--script for this page-->
        <script src="https://maps.googleapis.com/maps/api/js?key=<%- api_key %>"></script>
        <script src="/assets/js/gmaps/gmaps.js"></script>
        <% include ./partials/dsheet_scripts_tables %>
            <% include ./partials/data_sheet_details_scripts %>

</body>

</html>
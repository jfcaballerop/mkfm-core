<!-- Modal -->
<div class="modal fade" id="modalKobo" role="dialog">
    <div class="modal-dialog">

        <!-- Modal content-->
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Actualizar Kobo</h4>
            </div>
            <div class="modal-body">
                <div id="photo" class="col-sm-12">

                </div>
                <form class="form-horizontal style-form" method="post" action="/auth/WEB/users/update_user2">
                    <div class="form-group">
                        <div class="col-sm-12">
                            <input name="kobo[_id]" required class="form-control" type="hidden">
                        </div>

                        <label class="col-sm-2 col-sm-2 control-label">Type</label>
                        <div class="col-sm-4">
                            <input name="kobo[properties][kobo_type]" type="text" class="form-control" disabled>
                        </div>
                        <label class="col-sm-2 col-sm-2 control-label">koboid</label>
                        <div class="col-sm-4">
                            <input name="kobokobo[properties][_id]" type="text" class="form-control" disabled>
                        </div>
                        <label class="col-sm-2 col-sm-2 control-label">Coordinates</label>
                        <div class="col-sm-10">
                            <input name="kobokobo[geometry][coordinates]" type="text" class="form-control">
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="submit" class="btn btn-success"><i class="fa fa-check"></i> Modificar</button>
                        <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                    </div>
                </form>
            </div>
        </div>

    </div>
</div>
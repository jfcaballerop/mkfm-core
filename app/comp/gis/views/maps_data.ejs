                /*use this property to disbale soft wrap*/
                white-space: nowrap;
                /*To make extra certain,add this css property*/
                word-break: keep-all;
                background-color: white;
            }
        </style> -->
</head>

<body>

    <section id="container">
        <!-- **********************************************************************************************************************************************************
      TOP BAR CONTENT & NOTIFICATIONS
      *********************************************************************************************************************************************************** -->
        <!--header start-->
        <header class="header black-bg">
            <% include partials/header %>
        </header>
        <!--header end-->

        <!-- **********************************************************************************************************************************************************
      MAIN SIDEBAR MENU
      *********************************************************************************************************************************************************** -->
        <!--sidebar start-->
        <aside>
            <% include partials/left_menu %>

        </aside>
        <!--sidebar end-->

        <!-- **********************************************************************************************************************************************************
        ***** Google Maps      
        *********************************************************************************************************************************************************** -->
        <!--main content start-->
        <section id="main-content">
            <section class="wrapper site-min-height">
                <div class="row mt">
                    <div class="col-md-12">
                        <h3>
                            <i class="fa fa-angle-right"></i>
                            <%- trans('MAPSDATA_TITLE') %>
                        </h3>
                    </div>
                </div>
                <div class="row mt">
                    <div class="col-md-12">
                        <div class="content-panel">
                            <h4>
                                <i class="fa fa-angle-right">
                                    <%- trans('MAPSDATA_FILTER') %>
                                </i>
                            </h4>
                            <hr>
                            <div class="form-inline">
                                <div class="form-group">

                                    <select id="roads" class="form-control" style="width:auto;">
                                        <option value="0">-
                                            <%- trans('MAPSDATA_SELECT_ROAD') %> -</option>
                                        <% roadsvals.forEach(function(val, indexrv){ %>
                                            <option value="<%- val._id %>">
                                                <%= val.properties.name %>
                                            </option>

                                            <% }); %>

                                    </select>
                                    <select id="infodatas" class="form-control" style="width:auto;">
                                        <option value="0">-
                                            <%- trans('MAPSDATA_SELECT_TRACK') %> -</option>
                                        <% infodatasvals.forEach(function(val, indexrv){ %>
                                            <option value="<%- val._id %>">
                                                <%- moment(val.updated_at).format('DD-MM-YYYY HH:mm:ss') %> ::
                                                    <%- val.properties.name %>
                                            </option>

                                            <% }); %>

                                    </select>
                                </div>
                                <div class="form-group">

                                    <button id="editbutton" onclick="go_edit_road();" type="button" class="btn btn-primary" style="display: none;">
                                        <i class="fa fa-pencil-square-o">
                                            <%- trans('MAPSDATA_BUTTON_EDIT') %>
                                        </i>
                                    </button>
                                    <button id="editvideobutton" onclick="go_edit_video_road();" type="button" class="btn btn-theme04" style="display: none;">
                                        <i class="fa fa-video-camera">
                                            <%- trans('MAPSDATA_BUTTON_EDITVIDEO') %>
                                        </i>
                                    </button>
                                    <button id="versionbutton" type="button" class="btn btn-theme02" style="display: none;">
                                        <i class="fa fa-code-fork"></i>
                                        <%- trans('MAPSDATA_BUTTON_NEWVER') %>
                                    </button>
                                    <button id="downloadbutton" type="button" class="btn btn-theme03" style="display: none;">
                                        <i class="fa fa-download"></i>
                                        <%- trans('MAPSDATA_BUTTON_DOWN') %>
                                    </button>
                                    <button id="exportbutton" type="button" class="btn btn-success" style="display: none;">
                                        <i class="fa fa-file-excel-o"></i>
                                        <%- trans('MAPSDATA_BUTTON_EXPORT') %>
                                    </button>
                                    <span id="ultimaversion" style="display: none;"></span>
                                    <div>
                                        <div id="accordion" style="display:none;">
                                            <div class="table-responsive">
                                                <table id="dataTable" class="table table-bordered">

                                                    <tr>

                                                        <td>
                                                        </td>
                                                        <td>
                                                        </td>
                                                        <td>
                                                        </td>
                                                    </tr>

                                                    </tbody>
                                                </table>
                                            </div>
                                        </div>

                                    </div>
                                    <!-- form panel-->
                                </div>
                                <!--/content-panel -->
                            </div>
                        </div>
                        <!-- page end-->
            </section>
        </section>
        <!-- /MAIN CONTENT -->

        <!--main content end-->
        <!--footer start-->
        <footer class="site-footer">
            <% include partials/footer %>
        </footer>
        <!--footer end-->
    </section>

    <!-- js placed at the end of the document so the pages load faster -->
    <% include partials/footer_scripts %>

        <!--common script for all pages-->
        <script src="http://cdnjs.cloudflare.com/ajax/libs/raphael/2.1.0/raphael-min.js"></script>

        <script src="https://momentjs.com/downloads/moment-with-locales.min.js"></script>


        <!--script for this page-->

        <% include ./partials/maps_data_scripts %>

            <script src="/assets/js/jquery.table2excel.min.js"></script>

</body>

</html>
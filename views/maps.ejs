                                    return {
                                        fillColor: color,
                                        strokeColor: color,
                                        strokeWeight: 3,
                                        strokeOpacity: 0.7,
                                        icon: {
                                            path: google.maps.SymbolPath.CIRCLE,
                                            strokeColor: color,
                                            strokeOpacity: 0.5,
                                            scale: 3
                                        }
                                    }
                                });


